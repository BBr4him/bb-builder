{
  "name": "@bbfire/builders",
  "version": "1.0.0",
  "description": "my custom builder for angular fire",
  "builders": "./builders.json",
  "main": "index.js",
  "scripts": {
    "build": "tsc && ts-node build.ts && cd dist && npm pack && cd .. && ts-node copy-tgz.ts"
  },
  "author": "",
  "license": "no dc",
  "peerDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "@angular/common": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "firebase": "^7.13.1",
    "rxjs": "~6.6.0"
  },
  "devDependencies": {
    "ts-node": "^8.3.0",
    "typescript": "~4.0.2"
  },
  "dependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "tslib": "^2.0.0"
  },
  "ngPackage": {
    "lib": {
      "umdModuleIds": {
        "@angular/core": "ng",
        "@angular/common": "ng",
        "firebase/app": "firebase",
        "tslib": "tslib",
        "rxjs": "rxjs"
      },
      "entryFile": "./public_api.ts"
    },
    "dest": "../build"
  }
}
